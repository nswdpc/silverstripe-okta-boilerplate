{
    "name": "nswdpc/silverstripe-okta-boilerplate",
    "description": "An Okta authentication boilerplate for Silverstripe",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "James Ellis",
        "role": "Developer"
    }],
    "keywords": [
        "silverstripe",
        "authentication",
        "okta",
        "oauth2"
    ],
    "extra": {
        "expose": [
            "client/static"
        ]
    },
    "require": {
        "php": "^8.1",
        "bigfork/silverstripe-oauth-login": "^2.3",
        "foxworth42/oauth2-okta": "^1",
        "silverstripe/framework": "^5",
        "phptek/jsontext": "^2",
        "symbiote/silverstripe-queuedjobs": "^5"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3"
    },
    "autoload": {
        "psr-4": {
            "NSWDPC\\Authentication\\Okta\\": [
                "src/Controllers/",
                "src/Exceptions/",
                "src/Extensions/",
                "src/Handler/",
                "src/Jobs/",
                "src/Models/",
                "src/Services/",
                "src/Tasks/",
                "src/Traits/"
            ],
            "NSWDPC\\Authentication\\Okta\\Tests\\": "tests/"
        }
    }
}
